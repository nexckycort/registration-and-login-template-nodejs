{
  "name": "login-pg-nodejs",
  "version": "1.0.0",
  "author": "Nestor Cortina",
  "description": "",
  "main": "server.js",
  "engines": {
    "node": ">=14.15.3",
    "npm": ">=6.14.9"
  },
  "scripts": {
    "start": "node ./build/server.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/app.ts",
    "test": "cross-env NODE_ENV=test jest --forceExit --detectOpenHandles --coverage --verbose --testTimeout=10000",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "build": "webpack --config webpack.config.ts",
    "check-updates": "ncu",
    "tdocs": "typedoc --out docs ./src",
    "upgrade": "npm update --save-dev && npm update --save",
    "clean:pkg": "rimraf pkg",
    "build:pkg-linux": "mkdir pkg && pkg package.json --target node14-linux-x64 --output ./pkg/server",
    "build:pkg-win": "mkdir pkg && pkg package.json --targets node14-win-x64 --output ./pkg/server",
    "pkg:win": "npm-run-all build clean:pkg build:pkg-win",
    "pkg:linux": "npm-run-all build clean:pkg build:pkg-linux"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "bin": "./build/server.js",
  "pkg": {
    "scripts": "server.js"
  },
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.5.0",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@microsoft/tsdoc": "^0.13.0",
    "@tsconfig/node14": "^1.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.31",
    "@types/nodemailer": "^6.4.0",
    "@types/pg": "^7.14.10",
    "@types/supertest": "^2.0.10",
    "@types/webpack-node-externals": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-tsdoc": "^0.2.11",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "npm-check-updates": "^11.1.9",
    "npm-run-all": "^4.1.5",
    "pkg": "^4.4.9",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.20.28",
    "typescript": "^4.2.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  }
}
